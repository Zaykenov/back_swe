{
    "version": 2,
    "builds": [
      {
        "src": "backend/**",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "backend/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "backend.settings"
    },
    "build": {
      "env": {
        "DEBUG": "false"
      }
    }
  }
  